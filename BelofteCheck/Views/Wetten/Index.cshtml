@model IEnumerable<BelofteCheck.Wetten>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.WetNaam)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WetOmschrijving)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WetLink)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.WetNaam)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WetOmschrijving)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WetLink)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { WetID=item.WetID }) |
            @Html.ActionLink("Details", "Details", new { WetID=item.WetID }) |
            @Html.ActionLink("Delete", "Delete", new { WetID=item.WetID })
        </td>
    </tr>
}

</table>
